        <Slider x:ConnectionId='71' Style="{StaticResource SliderStyle1}" x:Name="volumeSlider" Width="250" HorizontalAlignment="Center" VerticalAlignment="Bottom" Minimum="0" Maximum="100" Value="50"/>
    </Grid>
</UserControl>

