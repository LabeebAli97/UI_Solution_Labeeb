        <Line x:ConnectionId='36' x:Name="f" Stroke="Gray" StrokeThickness="0.3" X1="172" X2="172" Y1="32" Y2="38"/>
        <Line x:ConnectionId='37' x:Name="g" Stroke="Gray" StrokeThickness="0.3" X1="178" X2="178" Y1="31.9" Y2="38.1"/>
